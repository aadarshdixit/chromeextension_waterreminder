{
    "manifest_version": 3,
    "name": "water remainder" ,
    "version": "0.0.1",
    "author": "aadarsh dixit",
  
    "action": {
      "default_popup": "popup.html",
      "default_icon": "logo.png",
      "default_title": "water remainder"
    },
    
    "icons": {
      "128": "logo.png"
    },
  
    "background": {
      "service_worker": "background.js"
      
    },
  
    "permissions": ["scripting", "tabs", "activeTab","bookmarks","alarms","notifications","storage"]
  
  }